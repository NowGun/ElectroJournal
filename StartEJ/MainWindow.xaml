<Window
    x:Class="StartEJ.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls1="clr-namespace:Notifications.Wpf.Controls;assembly=Notifications.Wpf"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:StartEJ"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wpfui="clr-namespace:WPFUI.Controls;assembly=WPFUI"
    Title="Первоначальная настройка ElectroJournal"
    Width="800"
    Height="600"
    Loaded="Window_Loaded"
    Style="{DynamicResource UiWindowRounded}"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid>
        <wpfui:TitleBar
            x:Name="TitleBar"
            Title="Первоначальная настройка ElectroJournal"
            Grid.Row="0"
            Height="48" />

        <wpfui:Card
            Width="700"
            Height="500"
            Margin="0,60,0,0"
            HorizontalAlignment="Center"
            VerticalAlignment="Top">
            <Grid>
                <Frame x:Name="FrameEJ" />

            </Grid>
        </wpfui:Card>
        <controls1:NotificationArea
            x:Name="WindowArea"
            Margin="0,53,10,0"
            MaxItems="1"
            Position="TopRight" />


    </Grid>
</Window>
