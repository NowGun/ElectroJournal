                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="dynamicScrollViewer" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="230.612" />
                <EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="card" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.15" />
                <EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="card" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="-25.51" />
                <EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="card" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="1" />
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="grid1" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0" />
                <EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="grid" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0" />
                <EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="GridStudentInfo" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00.3000000" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="GridStudentInfo" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00.3000000" Value="-15.3">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CubicEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="GridStudentInfo" Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00.3000000" Value="{x:Static Visibility.Collapsed}" />
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="AnimCloseLoad">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="grid" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:00.4000000" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="grid1" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:00.4000000" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="stackPanel" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00.3000000" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut" />
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="stackPanel" Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00.3000000" Value="{x:Static Visibility.Collapsed}" />
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>
    </Page.Resources>
    <Grid Margin="10,10,10,10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid
            x:Name="grid"
            Grid.Column="0"
            HorizontalAlignment="Left"
            RenderTransformOrigin="0.5,0.5">
            <Grid.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform />
                    <TranslateTransform />
                </TransformGroup>
            </Grid.RenderTransform>
            <ListBox
                x:Name="ListBoxStudent"
                MinWidth="275"
                Padding="2,50,2,0"
                SelectionChanged="ListBoxStudent_SelectionChanged" />
            <wpfui:SearchBox
                x:Name="SearchBoxStudents"
                Height="35"
                Margin="10,10,10,0"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Top"
                Placeholder="Поиск"
                PreviewKeyUp="SearchBoxStudents_PreviewKeyUp" />
        </Grid>
        <Grid x:Name="grid1" Grid.Column="1">
            <wpfui:Card
                x:Name="card"
                Height="60"
                Padding="6,6,6,6"
                VerticalAlignment="Top"
                RenderTransformOrigin="0.5,0.5">
                <wpfui:Card.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </wpfui:Card.RenderTransform>
                <Grid>
                    <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                        <Label
                            x:Name="LabelGroupName"
                            Margin="10,5,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Группа"
                            FontSize="22" />
                        <Menu
                            Height="55"
                            Margin="20,-4.5,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Background="Transparent"
                            BorderBrush="Transparent">
                            <MenuItem FontSize="16" Header="Отчеты">
                                <MenuItem
                                    x:Name="MenuItemReportScores"
                                    Click="MenuItemReportScores_Click"
                                    Header="Ведомость оценок" />
                            </MenuItem>
                        </Menu>
                    </StackPanel>
                    <StackPanel
                        x:Name="StackPanelGenerateSheet"
                        Margin="320,0,10,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Orientation="Horizontal"
                        Visibility="Visible">
                        <Label
                            x:Name="LabelScheduleGroup"
                            Margin="0,0,20,0"
                            Content="Физика\45 - Ларионова Ольга" />
                        <StackPanel Orientation="Horizontal" Visibility="Collapsed">
                            <Label Content="Генерация списка" />
                            <ProgressBar
                                x:Name="ProgressBarReport"
                                Margin="10,0,0,0"
                                Foreground="#FF3567BC"
                                IsIndeterminate="True"
                                Style="{StaticResource MaterialDesignCircularProgressBar}"
                                Visibility="Visible" />
                        </StackPanel>
                    </StackPanel>
                </Grid>
            </wpfui:Card>
            <wpfui:DynamicScrollViewer
                x:Name="dynamicScrollViewer"
                Margin="0,70,0,75"
                RenderTransformOrigin="0.5,0.5">
                <wpfui:DynamicScrollViewer.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </wpfui:DynamicScrollViewer.RenderTransform>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="29*" />
                        <RowDefinition Height="28*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <ListBox Grid.Row="0" Grid.Column="0" x:Name="ListBoxPresences">
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Label 
                                        VerticalAlignment="Top"
                                        Content="{Binding message}"
                                        FontSize="14" />
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                    </ListBox>
                    <ucontrol:LinesChartUC
                        Grid.Row="1"
                        Grid.ColumnSpan="2"
                        Margin="0,10,0,0" />
                    <ucontrol:PieChartsUC Grid.Row="0" Grid.Column="1" />
                </Grid>
            </wpfui:DynamicScrollViewer>
            <wpfui:CardAction
                x:Name="CardActionOpenStatsStud"
                Height="70"
                VerticalAlignment="Bottom"
                Click="CardActionOpenStatsStud_Click"
                Icon="PersonNote24"
                RenderTransformOrigin="0.5,0.5">
                <wpfui:CardAction.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </wpfui:CardAction.RenderTransform>
                <StackPanel>
                    <Label
                        x:Name="LabelFIOStudent"
                        Margin="0,0,0,4"
                        Content="Выберите студента из списка слева"
                        FontSize="14" />
                    <Label
                        Content="Нажмите чтобы посмотреть подробную информацию о студенте"
                        FontSize="11"
                        Foreground="{DynamicResource TextFillColorTertiaryBrush}" />
                </StackPanel>
            </wpfui:CardAction>
        </Grid>
        <wpfui:Dialog
            x:Name="RootDialogDiapDate"
            Grid.RowSpan="3"
            Grid.ColumnSpan="2"
            Margin="-10,-99,-10,-10"
            d:IsHidden="True"
            ButtonLeftClick="RootDialogNewSchedule_ButtonLeftClick"
            ButtonLeftName="Сгенерировать"
            ButtonRightClick="RootDialogNewSchedule_ButtonRightClick"
            ButtonRightName="Закрыть"
            DialogHeight="200"
            DialogWidth="400"
            IsShown="False">
            <Grid>
                <StackPanel>
                    <Label Margin="0,0,0,10" Content="Выберите диапазон дат" />
                    <StackPanel Orientation="Horizontal">
                        <DatePicker x:Name="DatePickerStart" Width="165" />
                        <Label
                            Margin="5,0,5,0"
                            VerticalAlignment="Center"
                            Content=" - " />
                        <DatePicker x:Name="DatePickerEnd" Width="165" />
                    </StackPanel>
                </StackPanel>
            </Grid>
        </wpfui:Dialog>
        <StackPanel
            x:Name="stackPanel"
            Grid.ColumnSpan="2"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            d:IsHidden="True">
            <ProgressBar
                x:Name="ProgressBar"
                Foreground="#FF3567BC"
                IsIndeterminate="True"
                Style="{StaticResource MaterialDesignCircularProgressBar}"
                Visibility="Visible" />
            <Label
                Margin="0,10,0,0"
                Content="Загрузка"
                FontSize="16" />
        </StackPanel>
        <Grid
            x:Name="GridStudentInfo"
            Grid.ColumnSpan="2"
            d:IsHidden="True"
            RenderTransformOrigin="0.5,0.5">
            <Grid.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform />
                    <TranslateTransform />
                </TransformGroup>
            </Grid.RenderTransform>
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Frame x:Name="FrameStud" Grid.RowSpan="2" />
            <StackPanel Orientation="Horizontal">
                <wpfui:SymbolIcon
                    x:Name="SymbolIconBack"
                    Grid.Row="0"
                    Margin="15,0,0,0"
                    HorizontalAlignment="Left"
                    Cursor="Hand"
                    FontSize="20"
                    PreviewMouseLeftButtonUp="SymbolIconBack_PreviewMouseLeftButtonUp"
                    Symbol="ArrowLeft24" />
            </StackPanel>
        </Grid>
    </Grid>
</Page>
